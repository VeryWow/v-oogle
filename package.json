{
  "name": "v-oogle",
  "version": "20180402.0.0",
  "description": "A main google page written in Vue,js",
  "author": "Raiondesu <a.raiondesu@gmail.com>",
  "scripts": {
    "dev": "webpack-dev-server --mode development --config config/webpack.dev.ts --open --hot",
    "build": "webpack --mode production --config config/webpack.prod.ts",
    "build-dev": "webpack --mode development --config config/webpack.prod.ts",
    "server": "http-server-spa dist index.html 5000",
    "start": "npm run build && npm run server",
    "start-dev": "npm run build-dev && npm run server",
    "prepush": "npm run build && (git add .) && git commit -m Build.",
    "test": "jest"
  },
  "homepage": "https://github.com/VeryWow/v-oogle#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VeryWow/v-oogle.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VeryWow/v-oogle/issues"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/webpack": "^4.1.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "element-ui": "^2.2.2",
    "html-webpack-plugin": "^3.0.7",
    "http-server-spa": "^1.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-vue-preprocessor": "^1.4.0",
    "node-sass": "^4.7.2",
    "pug": "^2.0.2",
    "sass-loader": "^6.0.7",
    "scss-loader": "0.0.1",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2",
    "vue-loader": "^14.2.1",
    "vue-simple-suggest": "^1.5.8",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "vue-test-utils": "^1.0.0-beta.11",
    "vue-typescript-import-dts": "^3.1.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-simple-events": "^1.1.2"
  },
  "keywords": [
    "boilerplate",
    "template",
    "modern",
    "simple",
    "Vue",
    "Vue.js",
    "WebPack",
    "webpack@4",
    "webpack-4",
    "typescript",
    "google-closure-compiler",
    "ts-node",
    "tests"
  ],
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.esm.js"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "testRegex": "/tests/.*\\.(ts|js)$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/.*\\.(ts|js)$",
      "/.*\\.vue$"
    ]
  }
}
